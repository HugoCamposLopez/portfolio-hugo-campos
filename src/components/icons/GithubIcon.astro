---
import GitHuSvg from "../../../public/icons/github-mark-white.svg"
import { Image } from 'astro:assets';

interface Props {
  width?: number,
  height?: number
}

const {height, width} = Astro.props
---
<a target="_blank" href="https://github.com/HugoCamposLopez" class="transition hover:scale-125">
  <Image id={"github-icon"} src={GitHuSvg} alt={"GithubIcon"}  width={width} height={height}/>
</a>

