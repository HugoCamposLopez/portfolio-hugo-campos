---
import GitHuSvg from "../assets/github-mark-white.svg"
type Props = {
  width?: number,
  height?: number
}

const {height, width } = Astro.props
---

    <a target="_blank" href="https://github.com/HugoCamposLopez" class="transition hover:scale-125" >
      <img id={"github-icon"} src={GitHuSvg.src} alt={"GithubIcon"} width={width} height={height} />
    </a >
  

