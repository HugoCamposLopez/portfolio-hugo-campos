---
import linkedinIcon from "../assets/linkedin-icon.png"

type Props = {
  width?: number,
  height?: number
}

const {height, width } = Astro.props
---

    <a target="_blank" href="https://www.linkedin.com/in/hugocamposlopez/" class="transition hover:scale-125">
      <img id={"linkeid-icon"} src={linkedinIcon.src} alt={"LinkedIn Icon"} width={width} height={height} />
    </a>



